<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="398.0" prefWidth="259.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RegisterController">
   <children>
      <TextField fx:id="usernameField" layoutX="22.0" layoutY="75.0" prefHeight="26.0" prefWidth="214.0" promptText="Username" />
      <Button fx:id="registerButton" layoutX="97.0" layoutY="264.0" mnemonicParsing="false" onMouseClicked="#onRegisterClicked" prefHeight="32.0" prefWidth="65.0" style="-fx-background-radius: 30;" text="Register" />
      <Button fx:id="backButton" layoutX="97.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#redirectLogin" prefHeight="32.0" prefWidth="65.0" style="-fx-background-radius: 30;" text="Back" />
      <TextField fx:id="displayNameField" layoutX="22.0" layoutY="199.0" promptText="Display Name" />
      <TextField fx:id="ageField" layoutX="197.0" layoutY="199.0" prefHeight="26.0" prefWidth="38.0" promptText="Age" />
   </children>
</AnchorPane>
